<form method="post" action="{{ path('app_paie_delete', {'id': paie.id}) }}"
      onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette paie ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ paie.id) }}">
    <button type="submit" class="btn btn-danger px-4">Supprimer</button>
</form>
