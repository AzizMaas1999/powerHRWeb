{% extends 'base.html.twig' %}

{% block title %}Jours Fériés{% endblock %}

{% block body %}
<h1>Jours Fériés en Tunisie</h1>

<table class="table">
    <thead>
        <tr>
            <th>Date</th>
            <th>Nom</th>
        </tr>
    </thead>
    <tbody>
        {% for holiday in holidays %}
            <tr>
                <td>{{ holiday.date }}</td>
                <td>{{ holiday.name }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>

<a href="{{ path('app_demande_new') }}" class="btn btn-primary">Retour</a>
{% endblock %}
